  "bundleDependencies": false,
  "deprecated": false,
  "description": "eferhatg.com",
  "devDependencies": {
    "autoprefixer": "^9.4.7",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015-ie": "^6.7.0",
    "babel-preset-minify": "^0.5.1",
    "bulma": "^0.7.2",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.1"
  },
  "homepage": "https://eferhatg.com",
  "license": "MIT",
  "name": "eferhatg.com",
  "repository": {
    "type": "git",
    "url": "git@github.com:eferhatg/eferhatg.github.io.git"
  },
  "scripts": {
    "css-build": "node-sass _sass/main.scss css/main.css --output-style compressed",
    "css-deploy": "npm run css-build && npm run css-postcss",
    "css-postcss": "postcss --use autoprefixer --output css/main.css css/main.css",
    "css-watch": "npm run css-build -- --watch",
    "deploy": "npm run css-deploy  && npm run js-build",
    "js-build": "babel _javascript --out-dir lib --presets minify",
    "js-watch": "npm run js-build -- --watch",
    "start": "npm-run-all --parallel css-watch js-watch"
  },
  "version": "0.0.3"
}
